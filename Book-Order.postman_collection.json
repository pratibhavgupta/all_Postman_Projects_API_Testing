			"name": "Authentication_Github",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const accessToken=pm.response.json().accessToken;\r",
							"\r",
							"pm.environment.set(\"accessToken\", accessToken);"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n        \"clientName\": \"{{clientName}}\",\r\n        \"clientEmail\": \"{{clientEmail}}\"\r\n      }",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{Base_Url}}/api-clients/",
					"host": [
						"{{Base_Url}}"
					],
					"path": [
						"api-clients",
